{
  "name": "nestjs-fireorm",
  "version": "0.0.0",
  "description": "Nestjs Fireorm module",
  "keywords": [
    "config",
    "env-var",
    "dotenv",
    "typescript"
  ],
  "main": "dist/lib/nestjs-fireorm.js",
  "typings": "dist/types/nestjs-fireorm.d.ts",
  "files": [
    "dist"
  ],
  "author": "glebbash <glebbash@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/glebbash/nestjs-fireorm"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "build:docs": "typedoc --out docs --theme minimal src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --config jest.config.ts --coverage",
    "test:watch": "jest --config jest.config.ts --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "@google-cloud/firestore": "^4.9.8",
    "@nestjs/common": "^7.6.15",
    "fireorm": "^0.20.0"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@google-cloud/firestore": "^4.9.8",
    "@nestjs/common": "^7.6.15",
    "@types/jest": "26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "firebase-admin": "^9.6.0",
    "fireorm": "^0.20.0",
    "husky": "^5.1.3",
    "jest": "26.6.3",
    "jest-config": "26.6.3",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.28",
    "typescript": "^4.1.3"
  }
}
