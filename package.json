{
  "name": "@skypress/nestjs-fireorm",
  "version": "0.1.0",
  "description": "Nestjs Fireorm module",
  "keywords": [
    "nestjs",
    "fireorm",
    "firestore",
    "firebase",
    "typescript"
  ],
  "main": "dist/lib/nestjs-fireorm.js",
  "typings": "dist/types/nestjs-fireorm.d.ts",
  "files": [
    "dist"
  ],
  "author": "BMO <bmo@skypress.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/SkyPressATX/nestjs-fireorm"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@google-cloud/firestore": "^5.0.0",
    "@nestjs/common": "^8.0.0",
    "fireorm": "^0.23.0"
  },
  "devDependencies": {
    "@google-cloud/firestore": "^5.0.0",
    "@nestjs/common": "^8.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-admin": "^10.0.0",
    "fireorm": "^0.23.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
